{
  "name": "multi_rtsp_to_filesystem",
  "root": true,
  "plugins": [
    {
      "id": "capture_multi",
      "kind": "capture_rtsp_multi",
      "cfg": {
        "streams": [
          {
            "stream_id": 0,
            "url": "rtsp://USERNAME:PASSWORD@IP_ADDRESS:554/Streaming/Channels/101?transportmode=mcast&profile=Profile_1",
            "transport": "tcp",
            "hw_decode": false,
            "max_retry_attempts": 5,
            "retry_delay_ms": 2000
          },
          {
            "stream_id": 1,
            "url": "rtsp://USERNAME:PASSWORD@IP_ADDRESS:554/Streaming/Channels/101?transportmode=mcast&profile=Profile_2",
            "transport": "tcp", 
            "hw_decode": false,
            "max_retry_attempts": 5,
            "retry_delay_ms": 2000
          }
        ]
      },
      "children": [
        {
          "id": "store_stream0",
          "kind": "store_filesystem",
          "cfg": {
            "root": "/tmp/zm-recordings/stream0",
            "max_secs": 300,
            "monitor_id": 0,
            "stream_filter": [0]
          }
        }
      ]
    }
  ]
}
